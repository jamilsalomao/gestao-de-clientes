{
  "expo": {
    "name": "gestao-de-clientes",
    "scheme": "your-app-scheme",
    "slug": "gestao-de-clientes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/FenixIcon.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fenix.gestordeclientes",
      "splash": {
      "image": "./assets/FenixLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/FenixIcon.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fenix.gestordeclientes",
      "splash": {                 
        "image": "./assets/FenixLogo.png", 
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
